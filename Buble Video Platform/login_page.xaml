<Page x:Class="Buble_Video_Platform.login_page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Buble_Video_Platform"
     Height="700" Width="1300"
      Title="login_page">

    <Grid Background="#292242">
        <TextBox x:Name="username_textbox" HorizontalAlignment="Left" Height="22" Margin="266,126,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="184"/>
        <TextBox x:Name="password_textbox" HorizontalAlignment="Left" Height="21" Margin="266,225,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="184"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="26" Margin="266,301,0,0" VerticalAlignment="Top" Width="184" Click="Button_Click"/>

    </Grid>
</Page>
